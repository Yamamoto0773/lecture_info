create_table :lectures, force: :cascade do |t|
  t.datetime :canceled_from
  t.datetime :canceled_to
  t.datetime :supplemented_from
  t.datetime :supplemented_to
  t.string  :subject, null: false
  t.string  :class_name
  t.string  :room
  t.string  :staff
  t.text    :remarks
  
  t.timestamps null: false
end
